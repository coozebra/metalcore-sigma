{
  "name": "sigma",
  "version": "1.7.0-jaa-mata",
  "private": true,
  "engines": {
    "node": "16.13.0"
  },
  "scripts": {
    "dev": "NEXT_PUBLIC_APP_VERSION=$npm_package_version cp .env .env.local && ts-node --project tsconfig.server.json src/server/index.ts",
    "build": "NEXT_PUBLIC_APP_VERSION=$npm_package_version next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:staging": "NEXT_PUBLIC_APP_VERSION=$npm_package_version cp .env.staging .env.local && next build && npm run build:server",
    "build:production": "NEXT_PUBLIC_APP_VERSION=$npm_package_version cp .env.production .env.local && next build && npm run build:server",
    "export": "NEXT_PUBLIC_APP_VERSION=$npm_package_version next export",
    "start": "npm run dev",
    "start:pino": "NODE_OPTIONS='-r next-logger' npm run dev",
    "lint": "next lint",
    "test": "NODE_ENV=test jest",
    "compile-contract-types": "typechain --target ethers-v5 --out-dir './src/contracts/types' './src/contracts/ABI/*.json'",
    "postinstall": "npm run compile-contract-types",
    "pretty": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@binance-chain/bsc-connector": "^1.0.0",
    "@bugsnag/js": "^7.16.7",
    "@bugsnag/plugin-express": "^7.18.0",
    "@bugsnag/plugin-react": "^7.16.7",
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@ethersproject/wallet": "^5.5.0",
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/onboarding": "^1.0.1",
    "@types/express": "^4.17.13",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/metamask": "^8.0.0-beta.0",
    "@web3-react/walletconnect": "^8.0.35-beta.0",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "axios": "^0.24.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.27.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.18.1",
    "formik": "^2.2.9",
    "geoip-lite": "^1.4.6",
    "js-cookie": "^3.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "next": "13.0.0",
    "next-logger": "^3.0.1",
    "pino": "^8.6.1",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^18.2.0",
    "react-google-recaptcha-v3": "^1.10.0",
    "react-indiana-drag-scroll": "^2.2.0",
    "react-tabs": "^5.1.0",
    "request-ip": "^3.3.0",
    "styled-components": "^5.3.3",
    "swr": "^1.0.1",
    "ts-node": "^10.9.1",
    "use-immer": "^0.7.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@typechain/ethers-v5": "^8.0.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/node": "16.11.9",
    "@types/react": "17.0.36",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "7.32.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.4.2",
    "pino-pretty": "^9.1.0",
    "prettier": "2.7.1",
    "typechain": "^6.0.5",
    "typescript": "4.5.2"
  }
}
